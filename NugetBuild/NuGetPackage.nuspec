<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Habitat.Core.TestingLibrary</id>
    <version>$version$</version>
    <title>Habitat.Core.TestingLibrary</title>
    <authors>Ben Peterson</authors>
    <owners>
    </owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Core library used to support unit tests for Habitat. Includes:
      - Fake HTTP handlers for testing web service calls
      - Mock file system
      - Mock system clock
      - Random value generator for testing database and mapping code
      - Helpers for testing code built on the Task Parallel Library
    </description>
    <summary>Core library used to support unit tests for Habitat.</summary>
    <releaseNotes>-Eliminated dependency on Habitat.Core</releaseNotes>
    <licenseUrl>http://choosealicense.com/licenses/gpl-v3/</licenseUrl>
    <copyright>2014</copyright>
    <language>en-US</language>
    <tags>Habitat Library</tags>
    <projectUrl>https://github.com/killthrush/Habitat.Core.TestingLibrary</projectUrl>
    <dependencies>
      <dependency id="Moq" version="[4.2.1402.2112]" />
      <dependency id="MSFT.ParallelExtensionsExtras" version="[1.2.0]" />
      <dependency id="Newtonsoft.Json" version="[6.0,7.0)" />
      <dependency id="SystemWrapper-sn" version="[0.5.3]" />
      <dependency id="Microsoft.Net.Http" version="[2.0.20710.0]" />
    </dependencies>
  </metadata>
  <files>
    <file src="$binfolder$Habitat.Core.TestingLibrary.dll" target="lib\net40" />
  </files>
</package>